{
    "LambdaFunctionConfigurations": [
        {
            "Id": "ThumbnailTrigger",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:550869127860:function:thumbnail-generator",
            "Events": ["s3:ObjectCreated:*"],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {"Name": "suffix", "Value": ".jpg"}
                    ]
                }
            }
        },
        {
            "Id": "ThumbnailTriggerPNG",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:550869127860:function:thumbnail-generator",
            "Events": ["s3:ObjectCreated:*"],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {"Name": "suffix", "Value": ".png"}
                    ]
                }
            }
        },
        {
            "Id": "ThumbnailTriggerJPEG",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:550869127860:function:thumbnail-generator",
            "Events": ["s3:ObjectCreated:*"],
            "Filter": {
                "Key": {
